// Codigo para exportar tabla a excel

$("#btnExportar").click(function(){
  $("#table_clase").table2excel({
    // exclude CSS class
    exclude: ".noExl",
    name: "Worksheet Name",
    filename: "SomeFile", //do not include extension
    fileext: ".xls" // file extension
  }); 
});

