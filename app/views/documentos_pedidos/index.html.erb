<%= form_tag documentos_pedidos_path, :method => 'get' do %>

<p id="notice"><%= notice %></p>

<h1>Documentos Pedidos</h1>

  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>

<div id="documentos_pedidos">

<table>
  <thead>
    <tr>
      <th>Numero Solicitud</th>
      <th>Nit</th>
      <th>Tercero</th>
      <th>Numero Clase</th>
      <th>Clase</th>
      <th>Iva</th>
      <th>Valor Total</th>
      <th>Fecha Hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos_pedidos.each do |documentos_pedido| %>
      <tr>
        <td><%= documentos_pedido.id %></td>
        <td><%= documentos_pedido.nit %></td>
        <td><%= documentos_pedido.tercero %></td>
        <td><%= documentos_pedido.id_clase %></td>
        <td><%= documentos_pedido.clase %></td>
        <td><%= documentos_pedido.iva %></td>
        <td><%= documentos_pedido.valor_total %></td>
        <td><%= documentos_pedido.created_at %></td>
        <td><%#= link_to 'Show', documentos_pedido %></td>
        <td><%#= link_to 'Edit', edit_documentos_pedido_path(documentos_pedido) %></td>
        <td><%#= link_to 'Destroy', documentos_pedido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Productos', documentos_pedido_documentos_lin_pedidos_path(documentos_pedido) %></td>
        <td><%= link_to 'Excel', documentos_pedido_documentos_lin_pedidos_path(documentos_pedido, format: "csv") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<%= link_to "Atras", usuario_usuarios_clases_path(current_user.id) %>

<%= will_paginate @documentos_pedidos, :previous_label => 'Anterior', :next_label => 'Siguiente' %>

<% end %>
<%#= link_to 'New Documentos Pedido', new_documentos_pedido_path %>
