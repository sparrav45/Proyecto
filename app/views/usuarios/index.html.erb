<%= form_tag usuarios_path, :method => 'get' do %>

<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>
<%#= @usuario_session.cedula %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>


<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.cedula %></td>
        <td><%= usuario.nombre %></td>
        <td><%= usuario.apellido %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.password %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Clases', usuario_usuarios_clases_path(usuario) %></td>
        <td><%#=link_to 'New Documentos Pedido', new_usuario_documentos_pedido_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Usuario', new_usuario_path %>

<%= will_paginate @usuarios, :previous_label => 'Anterior', :next_label => 'Siguiente' %>

<% end %>
