// Codigo para exportar tabla a excel

$("#btnExportar").click(function(){
  $("#table_clase").table2excel({
    // exclude CSS class
    exclude: ".noExl",
    name: "Worksheet Name",
    filename: "SomeFile", //do not include extension
    fileext: ".xls" // file extension
  }); 
});

//Funcion para colorear los registro dependiente de la condiconal y los valores que alla el campo d√≠a
 $(document).ready(function(){
  
            // Obtenemos todos los valores contenidos en los <td> de la fila
            // seleccionada
            $('#lotetabla tr').each(function(){

                let dialote=$(this).find('td').eq(6).text();

                if (dialote <=90) {
                  $(this).css("background-color", "#F05E27")

                }else if(dialote >91 && dialote <=180 ){
                      $(this).css("background-color", "#FCF63F")
                    
                  }
            });
            
});